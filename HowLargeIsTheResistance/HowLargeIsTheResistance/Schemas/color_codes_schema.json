{
  "$id": "http://HowLargeIsTheResistance.com/Color_codes_schema.json",
  "$schema": "http://json-schema.org/draft-04/schema",
  "definitions": {},
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "Color",
      "DecimalValue",
      "UnitValue",
      "Multiplier",
      "Tolerance"
    ],
    "properties": {
      "Color": {
        "$id": "#/items/properties/Color",
        "type": "string",
        "title": "The Color Schema",
        "default": "",
        "examples": [
          "keine"
        ],
        "pattern": "^(.*)$"
      },
      "DecimalValue": {
        "$id": "#/items/properties/DecimalValue",
        "type": "string",
        "title": "The Decimal Schema",
        "default": "",
        "examples": [
          "not_allowed"
        ],
        "pattern": "^(.*)$"
      },
      "UnitValue": {
        "$id": "#/items/properties/UnitValue",
        "type": "string",
        "title": "The Unit_position Schema",
        "default": "",
        "examples": [
          "not_allowed"
        ],
        "pattern": "^(.*)$"
      },
      "Multiplier": {
        "$id": "#/items/properties/Multiplier",
        "type": "string",
        "title": "The Multiplier Schema",
        "default": "",
        "examples": [
          "not_allowed"
        ],
        "pattern": "^(.*)$"
      },
      "Tolerance": {
        "$id": "#/items/properties/Tolerance",
        "type": "string",
        "title": "The Tolerance Schema",
        "default": "",
        "examples": [
          "20"
        ],
        "pattern": "^(.*)$"
      }
    }
  },
  "title": "The Root Schema",
  "type": "array"
}
